{
    "name": "dhii/output-renderer-base",
    "description": "Base concrete functionality for output rendering.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "dhii/output-renderer-interface": "^0.2 | ^0.3-alpha1",
        "dhii/output-renderer-abstract": "^0.1-alpha1",
        "dhii/exception": "^0.1-alpha4",
        "dhii/container-helper-base": "^0.1-alpha5",
        "dhii/i18n-helper-base": "^0.1-alpha1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "dhii/stringable-interface": "^0.1",
        "psr/container": "^1.0"
    },
    "suggested": {
        "dhii/stringable-interface": "To use stringable objects instead of strings"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Output\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}
